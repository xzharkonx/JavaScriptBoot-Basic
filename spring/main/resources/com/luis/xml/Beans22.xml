<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

<bean id="Jugador22" class="com.luis.beans.Jugador22">
<property name="id" value="1"></property>
<property name="nombre" value="Lionel Messi"></property>
<property name="equipo" ref="barcelona"></property>
<property name="camiseta" ref="camiseta"></property>
</bean>

<bean id="barcelona" class="com.luis.beans.Barcelona18"></bean>
<bean id="juventus" class="com.luis.beans.Juventus18"></bean>

<bean id="camiseta" class="com.luis.beans.Camiseta22">
<property name="id" value="1"></property>
<property name="numero" value="10"></property>
<property name="marca" ref="marca"></property>
</bean>

<bean id="marca" class="com.luis.beans.Marca22">
<property name="id" value="1"></property>
<property name="nombre" value="Gucci"></property>
</bean>
</beans>
